<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%= title %></title>
    <link href="../style.css" rel="stylesheet" />
    <script src="../index.js" defer="defer"></script>
  </head>

  <body>
    <div class="container">
      <%- include('header.ejs') %>

      <main class="main gal-main" data-name="insects-gallery">
        <!-- INSECTS GALLERY -->
        <section class="tab-content gal-portrait" id="insects-gallery" hidden>
          <div class="gallery portrait-gallery">
            <div class="animation-ctrl-div portrait-animation-ctrl-div">
              <button
                type="button"
                class="pause-btn portrait-pause-btn"
                title="Press Enter or Space to Pause Gallery Animation"
              >
                <img
                  class="pause-icon portrait-pause-icon"
                  src="../assets/imgs/pause-button-icon.svg"
                  alt="Press Enter or Space to Pause Gallery Animation"
                />
              </button>

              <button
                type="button"
                class="play-btn portrait-play-btn"
                title="Press Enter or Space to Start Gallery Animation"
              >
                <img
                  class="play-icon portrait-play-icon"
                  src="../assets/imgs/play-button-icon.svg"
                  title="Start Gallery Animation"
                  alt="Press Enter or Space to Start Gallery Animation"
                />
              </button>
            </div>

            <section
              class="slider-box portrait-slider-box"
              tabindex="0"
              role="region"
              aria-roledescription="photo slider"
              aria-label="Insects Gallery"
              hidden
            >
              <!-- <div class="slider-row-one"></div> -->
              <button type="button" class="back-arrow" data-back-arrow>
                <img
                  data-icon-back
                  class="arrow"
                  src="../assets/imgs/reshot-icon-arrow-chevron-left-975UQXVKZF.svg"
                  alt="Back Arrow"
                />
              </button>

              <div class="photo-box portrait-photo-box">
                <!-- BEE ON SAGE FLOWER -->
                <picture
                  class="picture"
                  aria-role="group"
                  aria-roledescription="slide"
                  aria-labelledby="slide1"
                  tabindex="0"
                >
                  <source
                    media="(min-width: 300px) and (max-width: 699px) and (orientation: portrait)"
                    srcset="
                      ../assets/imgs/insects-gal/Guneratne_2023.06.09_bee-vertical-320p.jpg
                    "
                  />

                  <source
                    media="(min-width: 700px) and (max-width: 799px) and (orientation: portrait)"
                    srcset="
                      ../assets/imgs/insects-gal/Guneratne_2023.06.09_bee-vertical-460p.jpg
                    "
                  />

                  <source
                    media="(min-width: 800px) and (max-width: 1020px) and (orientation: portrait)"
                    srcset="
                      ../assets/imgs/insects-gal/Guneratne_2023.06.09_bee-vertical-580p.jpg
                    "
                  />

                  <img
                    src="../assets/imgs/insects-gal/Guneratne_2023.06.09_bee-vertical-700p.jpg"
                    alt="Slide 1 of 6: A bee sitting on a purple sage flower with its arms over its head, as if about to thrust its arms forward onto the flower."
                    id="slide1"
                  />
                </picture>

                <!-- CATERPILLAR -->
                <picture
                  class="picture"
                  role="group"
                  aria-roledescription="slide"
                  aria-labelledby="slide2"
                  tabindex="0"
                >
                  <source
                    media="(min-width: 300px) and (max-width: 699px) and (orientation: portrait)"
                    srcset="
                      ../assets/imgs/insects-gal/Guneratne_2023.09.05_caterpillar-vertical-320p.jpg
                    "
                  />

                  <source
                    media="(min-width: 700px) and (max-width: 799px) and (orientation: portrait)"
                    srcset="
                      ../assets/imgs/insects-gal/Guneratne_2023.09.05_caterpillar-vertical-460p.jpg
                    "
                  />

                  <source
                    media="(min-width: 800px) and (max-width: 1020px) and (orientation: portrait)"
                    srcset="
                      ../assets/imgs/insects-gal/Guneratne_2023.09.05_caterpillar-vertical-580p.jpg
                    "
                  />

                  <img
                    src="../assets/imgs/insects-gal/Guneratne_2023.09.05_caterpillar-vertical-700p.jpg"
                    alt="Slide 2 of 6: A close up of a swallowtail butterfly caterpillar crawling vertically up a fennel stalk. The caterpillar is green with black stripes and yellow dots on every other stripe."
                    id="slide2"
                  />
                </picture>

                <!-- DRAGONFLY -->
                <picture
                  class="picture"
                  role="group"
                  aria-roledescription="slide"
                  aria-labelledby="slide3"
                  tabindex="0"
                >
                  <source
                    media="(min-width: 300px) and (max-width: 699px) and (orientation: portrait)"
                    srcset="
                      ../assets/imgs/insects-gal/Guneratne_2023.08.02_dragonfly-vertical-320p.jpg
                    "
                  />

                  <source
                    media="(min-width: 700px) and (max-width: 799px) and (orientation: portrait)"
