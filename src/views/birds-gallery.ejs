<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width" />
    <!-- Removed initial-scale and max-width from content attribute to fix zooming problems on mobile -->
    <title><%= title %></title>
    <link href="@styles/body-styles.css" rel="stylesheet" />
    <link href="@styles/slider-styles.css" rel="stylesheet" />
  </head>

  <body>
    <div class="scroll-wrapper">
      <main class="main gal-main" data-name="birds-gallery">
        <%- include('header.ejs') %>
        <!-- BIRDS GALLERY -->

        <section class="section" id="birds-gallery">
          <div class="gallery">
            <div
              class="slider-box"
              tabindex="0"
              role="region"
              aria-roledescription="photo slider"
              aria-label="Birds Gallery"
            >
              <%- include ('animation-ctrl.ejs') %>
              <button type="button" class="back-arrow" data-back-arrow>
                <img
                  data-icon-back
                  class="arrow"
                  src="@images/icons/reshot-icon-arrow-chevron-left-975UQXVKZF.svg"
                  alt="Back Arrow"
                />
              </button>

              <div class="photo-box">
                <!-- {MAY 2025 UPDATE} FEMALE HOUSE FINCH ON RADISH PLANT -->
