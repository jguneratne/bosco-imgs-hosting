                    srcset="<%=require('../src/assets/imgs/first-gal/Guneratne_2023.08.20_seagull-horizontal-600p.jpg') %>"
                  />

                  <img
                    src="<%=require('../src/assets/imgs/first-gal/Guneratne_2023.08.20_seagull-horizontal-900p.jpg') %>"
                    alt="Slide 5 of 6: Seagull at the ocean."
