              </div>

              <button type="button" class="forward-arrow" data-forward-arrow>
                <img
                  data-icon-forward
                  class="arrow"
                  src="<%=require('../src/assets/imgs/reshot-icon-arrow-chevron-right-WDGHUKQ634.svg') %>"
                  alt="Forward Arrow"
                />
              </button>

              <div class="img-dots-container" data-name="first-gallery">
                <div class="img-dots" data-name="first-gallery">
                  <button
                    type="button"
                    class="circle-icon-div"
                    data-circle="0"
                    data-name="first-gallery"
                    aria-label="Select Slide 1 of 6: Bunny eating long grass."
                  >
                    <div
                      class="circle-icon"
                      data-circle="0"
                      data-name="first-gallery"
                    ></div>
                  </button>

                  <button
                    type="button"
                    class="circle-icon-div"
                    data-circle="1"
                    data-name="first-gallery"
                    aria-label="Select Slide 2 of 6: Honeybee on a sage flower."
                  >
                    <div
                      class="circle-icon"
                      data-circle="1"
                      data-name="first-gallery"
                    ></div>
                  </button>

                  <button
                    type="button"
                    class="circle-icon-div"
                    data-circle="2"
                    data-name="first-gallery"
                    aria-label="Select Slide 3 of 6: Bird on a tree branch."
                  >
                    <div
                      class="circle-icon"
                      data-circle="2"
                      data-name="first-gallery"
                    ></div>
                  </button>

                  <button
                    type="button"
                    class="circle-icon-div"
                    data-circle="3"
                    data-name="first-gallery"
                    aria-label="Select Slide 4 of 6: Grasshopper on a leaf."
                  >
