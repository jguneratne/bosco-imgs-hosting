                  />

                  <source
                    media="(1280px <= width <= 1599px) and (orientation: landscape)"
                    srcset="<%=require('../src/assets/imgs/first-gal/Guneratne_2023.06.29_bird-horizontal-600p.jpg') %>"
                  />

                  <img
                    src="<%=require('../src/assets/imgs/first-gal/Guneratne_2023.06.29_bird-horizontal-900p.jpg') %>"
                    alt="Slide 3 of 6: Bird on a tree branch."
                    id="slide3"
                  />
                </picture>

                <!-- GRASSHOPPER -->
                <picture
                  aria-roledescription="slide"
                  aria-labelledby="slide4"
                  tabindex="0"
                >
                  <source
                    media="(300px <= width <= 699px) and (orientation: portrait)"
                    srcset="<%=require('../src/assets/imgs/first-gal/Guneratne_2023.08.26_grasshopper-vertical-320p.jpg') %>"
                  />

                  <source
                    media="(700px <= width <= 799px) and (orientation: portrait)"
                    srcset="<%=require('../src/assets/imgs/first-gal/Guneratne_2023.08.26_grasshopper-vertical-460p.jpg') %>"
