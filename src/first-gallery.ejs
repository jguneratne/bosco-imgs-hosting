                </picture>

                <!-- CATERPILLAR -->
                <picture
                  aria-roledescription="slide"
                  aria-labelledby="slide6"
                  tabindex="0"
                >
                  <source
                    media="(300px <= width <= 699px) and (orientation: portrait)"
                    srcset="<%=require('../src/assets/imgs/first-gal/Guneratne_2023.09.05_caterpillar-vertical-320p.jpg') %>"
                  />

                  <source
